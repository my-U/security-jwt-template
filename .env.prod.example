# .env.prod.example
# ⚠ 이 파일은 예시입니다.
# '.env.prod'로 이름을 바꾸고
# 프로젝트에 맞게 값을 입력하여 사용하세요.

# MySQL
SPRING_DATASOURCE_URL_MYSQL=jdbc:mysql://mysql:3306//example
SPRING_DATASOURCE_USERNAME_MYSQL=username
SPRING_DATASOURCE_PASSWORD_MYSQL=password

# PostgreSQL
SPRING_DATASOURCE_URL_POSTGRESQL=jdbc:postgresql://postgres:5432/example
SPRING_DATASOURCE_USERNAME_POSTGRESQL=username
SPRING_DATASOURCE_PASSWORD_POSTGRESQL=password

# Redis
SPRING_REDIS_HOST=redis


# Spring
SPRING_PROFILES_ACTIVE=prod



